## Note: The datastore is expected to be MySQL and have a database named
##   loadbalancing

#Datastore Configuration
datastore_host = 0.0.0.0
datastore_username = loadbalancing
datastore_password = password

#Showing the SQL will output Hibernate generated SQL at runtime
datastore_show_sql = false


#LB device WebService Configration
lbdevice_endpoint_uri = https://endpoint.lbdevicehost.com:9090/config
lbdevice_username = username
lbdevice_password = password
